{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "target": "ES2022",
        "jsx": "react",
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "skipLibCheck": true,
        // "sourceMap": true,
        "outDir": "./out"
    },
    "include": ["./src/**/*"],
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}